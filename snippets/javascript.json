{	
	"AddPrivateProperty": {
		"prefix": "child",
		"body": [
			"this._${1/.*/${0:/camelcase}/} = this.Children('${2:$1}$0');"
		],
		"description": "Add private property"
	},
	"AddEventHandlerReference": {
		"prefix": "event",
		"body": [
			"this._$1.AddHandler('$2', (event, args) => this.__$1$2(event, args));$0"
		],
		"description": "Add event handler"
	},
	"AddEventHandler": {
		"prefix": "handler",
		"body": [
			"/**",
			" * @private",
			" * $3",
			" */",
			"__$1(event, args) {",
			"\t$2",
			"}$0"
		],
		"description": "Add event handler"
	},
	"AddAttributeGet": {
		"prefix": "getattr",
		"body": [
			"/**",
			" * $3",
			" * @type {$2}",
			" */",
			"get $1() {",
			"\treturn this._$1;",
			"}$0"
		],
		"description": "Add attribute get"
	},
	"AddAttribute": {
		"prefix": "attr",
		"body": [
			"/**",
			" * $3",
			" * @type {$2}",
			" */",
			"get $1() {",
			"\treturn this._$1;",
			"}",
			"/**",
			" * $3",
			" * @type {$2}",
			" */",
			"set $1(value) {",
			"\tthis._$1 = value;",
			"\tthis._show${1/.*/${0:/capitalize}/}();",
			"}",
			"_show${1/.*/${0:/capitalize}/}() {",
			"\t$4",
			"}$0"
		],
		"description": "Add attribute"
	},
	"AddAttributeValue": {
		"prefix": "attrval",
		"body": [
			"/**",
			" * Value ${2:$1}",
			" * @type {${1|Object,Array,Number,String|}}",
			" */",
			"get value() {",
			"\treturn this._value;",
			"}",
			"/**",
			" * Value ${2:$1}",
			" * @type {${1|Object,Array,Number,String|}}",
			" */",
			"set value(value) {",
			"\tthis._value = value;",
			"\tthis._showValue();",
			"}",
			"_showValue() {",
			"\t$3",
			"}$0"
		],
		"description": "Add attribute"
	},
	"AddBinding": {
		"prefix": "binding",
		"body": [
			"/**",
			" * Render bounded to component data",
			" * @protected",
			" * @param {*} data ",
			" * @param {String} path ",
			" */",
			"__renderBoundedValues(data, path) {",
			"\tif(!data) {",
			"\t\treturn;",
			"\t}",
			"\t$1",
			"}$0"
		],
		"description": "Add event handler"
	},
	"multilangtext": {
		"prefix": "#",
		"body": [
			"#{$1}$0"
		],
		"description": "Add multilangual text"
	},
	"registerEventsMethod": {
		"prefix": "regm",
		"body": [
			"/**",
			" * Register events",
			" * @protected",
			" */",
			"_registerEvents() {",
			"\tsuper._registerEvents();",
			"\tthis.RegisterEvent('$1', ${2|false,true|}, '$3');",
			"}$0"
		],
		"description": "Add register events method"
	}
}